<%= paginator.render do -%>
  <nav>
    <ul class="pagination">
      <%# <%= first_page_tag %1> %>
      <%= prev_page_tag %>
      <% each_page do |page| -%>
        <% if page.left_outer? || page.right_outer? || page.inside_window? -%>
          <%= page_tag page %>
        <% elsif !page.was_truncated? -%>
          <%= gap_tag %>
        <% end -%>
      <% end -%>
      <%= next_page_tag %>
      <%# <%= last_page_tag %1> %>
    </ul>
  </nav>
<% end -%>
